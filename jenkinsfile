pipeline{
    agent any
    stages {
        stage("clean") {
            steps {
                deleteDir()
            }
        }
        stage('clone repo'){
            steps{
                bat "git clone https://github.com/liran774/test.git"
            }
        }
        stage('end of pipe'){
            steps{
                echo 'end of pipeline'
            }
            
        }
        stage('docker build'){
            steps{
                script{
                bat 'docker build -f test .'
            }
            }
            
        }
        }
    }
