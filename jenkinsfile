pipeline{
    agent any
    stages {
        stage("clean") {
            steps {
                deleteDir()
            }
        }
        stage('clone repo'){
            steps{
                bat "git clone https://github.com/liran774/python_test.git"
            }
        }
        stage('end of pipe'){
            steps{
                echo 'end of pipeline'
            }
            
        }
        stage('docker build'){
            scripts{
                bat 'docker build -t liran .'
            }
        }
        }
    }
